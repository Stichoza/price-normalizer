{
	"manifest_version": 2,
	"name": "Price Normalizer",
	"description": "Convert odd prices to real prices on shopping web sites",
	"version": "1.0.0",
	"icons": {
		"16":  "assets/icons/icon-16.png",
		"48":  "assets/icons/icon-48.png",
		"128": "assets/icons/icon-128.png"
	},
	"permissions": [
		"tabs",
		"*://*/*"
	],
	"content_scripts": [{
		"matches": ["*://*/*"],
		"js": ["price-normalizer.js"],
		"run_at": "document_end"
	}]

}
